<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
        <attr name="round_type" format="integer">
            <enum name="rect" value="0" />
            <enum name="round" value="1" />
            <enum name="circle" value="2" />
        </attr>
        <attr name="radius_size" format="dimension" />
        <attr name="top_left_rx" format="dimension" />
        <attr name="top_left_ry" format="dimension" />
        <attr name="top_right_rx" format="dimension" />
        <attr name="top_right_ry" format="dimension" />
        <attr name="bottom_right_rx" format="dimension" />
        <attr name="bottom_right_ry" format="dimension" />
        <attr name="bottom_left_rx" format="dimension" />
        <attr name="bottom_left_ry" format="dimension" />
        <attr name="ratio_w_h_reference" format="integer">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="ratio_w_h_size" format="float" />
        <attr name="stroke_width" format="dimension" />
        <attr name="stroke_color" format="color" />
    </declare-styleable>
</resources>